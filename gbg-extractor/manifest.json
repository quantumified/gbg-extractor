{
  "manifest_version": 3,
  "name": "FOE GBG Sector Extractor",
  "description": "Extracts Guild Battleground sector data and sends it to a Discord bot.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "background"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.forgeofempires.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
  "default_popup": "popup.html",
  "default_icon": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}

  }
}
